{
  "services": [
    {
      "name": "db-service",
      "id": "styx-db-1",
      "address": "styx-db-1",
      "port": 5432,
      "checks": [
        {
          "tcp": "styx-db-1:5432",
          "interval": "5s"
        }
      ]
    },
    {
      "name": "db-server",
      "address": "styx-db-1",
      "port": 80,
      "checks": [
        {
          "tcp": "styx-db-1:80",
          "interval": "5s"
        }
      ]
    }
  ]
}
